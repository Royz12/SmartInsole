<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.graduationdesign.springbootsmartinsole.mapper.ExpertInfoMapper">
    <insert id="insert">
        insert into expertinfo(expert_id,password,expertname,phonenumber,height,weight,worktime,selfintro,profile_picture)
        values(#{expert_id},#{password},#{expertname},#{phonenumber},#{height},#{weight},#{worktime},#{selfintro},#{profile_picture})
    </insert>

</mapper>
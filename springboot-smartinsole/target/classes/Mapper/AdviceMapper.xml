<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.graduationdesign.springbootsmartinsole.mapper.AdviceMapper">


    <insert id="insert">
        insert into advice(sportman_id,sportman_name,expert_id,expert_name,advice)
        values (#{sportman_id},#{sportman_name},#{expert_id},#{expert_name},#{advice})
    </insert>
</mapper>
<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.graduationdesign.springbootsmartinsole.mapper.SportmanInfoMapper">


    <insert id="insert">
        insert into sportmaninfo (sportman_id,password,sportman_name,phonenumber,height,weight,selfintro,profile_picture)
        values (#{sportman_id},#{password},#{sportman_name},#{phonenumber},#{height},#{weight},#{selfintro},#{profile_picture})
    </insert>
</mapper>
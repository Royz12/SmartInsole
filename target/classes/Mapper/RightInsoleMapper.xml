<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.graduationdesign.springbootsmartinsole.mapper.RightInsoleMapper">


    <select id="selectById" resultType="com.graduationdesign.springbootsmartinsole.entity.RightInsole">
        select * from rightinsole
        where rightid=#{right_id}
        order by id desc limit 1
    </select>
</mapper>
